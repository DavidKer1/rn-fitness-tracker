{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/adapters/*": ["adapters/*"],
			"@/assets/*": ["assets/*"],
			"@/components/*": ["components/*"],
			"@/constants/*": ["constants/*"],
			"@/context/*": ["context/*"],
			"@/hooks/*": ["hooks/*"],
			"@/locales/*": ["locales/*"],
			"@/models/*": ["models/*"],
			"@/navigation/*": ["navigation/*"],
			"@/redux/*": ["redux/*"],
			"@/screens/*": ["screens/*"],
			"@/services/*": ["services/*"],
			"@/styled-components/*": ["styled-components/*"],
			"@/theme/*": ["theme/*"],
			"@/utilities/*": ["utilities/*"]
		}
	}
}
